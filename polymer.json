{
  "root": "public",
  "entrypoint": "index.html",
  "shell": "web-components-primer.html",
  "sources": [
    "web-components-primer.js",
    "web-components-primer.html",
    "index.html",
    "styles.css",
    "styles.js"
  ],
  "extraDependencies": [
    "manifest.json",
    "images/**",
    "data/**",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "name": "esm-unbundled",
      "browserCapabilities": [ "es2015", "modules" ],
      "js": { "minify": true },
      "css": { "minify": true },
      "html": { "minify": true },
      "addServiceWorker": true,
      "swPrecacheConfig": "../sw-precache-config.js"
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
