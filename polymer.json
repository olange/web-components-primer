{
  "root": "public",
  "entrypoint": "index.html",
  "shell": "web-components-primer.html",
  "sources": [
    "*.js",
    "*.html",
    "*.css"
  ],
  "extraDependencies": [
    "manifest.json",
    "images/**",
    "data/**",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [ "es2015", "modules" ],
      "js": { "minify": true },
      "css": { "minify": true },
      "html": { "minify": true },
      "addServiceWorker": true,
      "swPrecacheConfig": "../sw-precache-config.js"
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [ "es2015" ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": { "minify": true },
      "html": { "minify": true },
      "addServiceWorker": true,
      "swPrecacheConfig": "../sw-precache-config.js"
    },
    {
      "name": "es5-bundled",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": { "minify": true },
      "html": { "minify": true },
      "addServiceWorker": true,
      "swPrecacheConfig": "../sw-precache-config.js"
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
